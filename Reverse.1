%{
#include <stdio.h>
#include <string.h>

void reverse(char *str) {
    int len = strlen(str);
    for(int i = 0; i < len / 2; ++i) {
        char temp = str[i];
        str[i] = str[len - i - 1];
        str[len - i - 1] = temp;
    }
}
%}

%%

[[:alnum:]]+     {
                    reverse(yytext);
                    printf("%s ", yytext);
                 }

[ \t\n]          { printf("%s", yytext); }

.                { printf("%s", yytext); }

